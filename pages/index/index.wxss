
/*轮播图区域样式*/
.carousel-item{
  width: 100%;
  height: 400rpx;
}
.swiper {
  height: 400rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
/* 布局分区域 */
.wrapper {
  height: 200rpx;
}
.regional-treatment{
  position: relative;
  top: -100rpx;
  width: 660rpx;
  height: 300rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  background: #fff;
  box-shadow: 0 0 20rpx #b1b0b0;
}
.date-house{
  height: 200rpx;
  background-color: green;
}
/* 表格样式 */
.table {
  width: 560rpx;
  margin: 0 auto;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 80rpx;
  align-items: center;
}
.td {
  color: 	#696969;
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background:#fff;
}
.bg-g{
  background: #FFFFFF;
  font-size: 40rpx;
}
.bg-z{
  background: #FFFFFF;
  
}
.th {
  width: 40%;
  justify-content: center;
  color: 	#696969;
  display: flex;
  height: 3rem;
  align-items: center;
  font-size: 30rpx;
 
}
/* 搜索按钮样式 */
.search-button{
  width: 530rpx;
  height: 70rpx;
  margin: 30rpx auto 0;
  line-height: 70rpx;
  font-size: 30rpx;
  background-color: #71C671;
  color: #fff;
}
.icon-agin{
  margin-right: 5%;
}
.icon-shape{
  width: 25rpx;
  height: 16rpx;
}
/* 房源区域 */
.house-regional-treatment{
  width: 100%;
  height: 400rpx;
  margin: 25rpx 25rpx 0 25rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
  margin-top: 25rpx;
}
.homeImg {
  width: 700rpx;
  height: 320rpx;
}
.mangFangStyle {
  margin-top: 10rpx;
  margin-left: 10rpx;
  width: 100rpx;
  height: 32rpx;
}
.house-text-treatment{
  display: flex;
  width: 100%;
  height: 40rpx;
  
}
.housePrice {
  margin-right: 10rpx;
}
.house-tile-font{
  margin-top: 6rpx;
  font-size: 30rpx;
}
.house-particulars-font{
  margin-top: 1%;
  font-size: 25rpx;
  color:#808080;
  
}
.text-fontwight{
  font-weight: bold;
}

.team {
  margin-left: 25rpx;
  margin-right: 25rpx;
  margin-top: 40rpx;
}

.teamText {
  display: flex;
  font-family: PingFangSC-Regular;
  color: #333;
  margin-top: 60rpx;
  margin-bottom: 40rpx;
}
.teamText image {
  width: 10rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.teamImg {
  width: 700rpx;
}

.dayWrapper {
  position: relative;
}
.dayContainer {
  width: 200rpx;
  height: 50rpx;
}

.liveDayMount {
  position: absolute;
  top: 2rpx;
  left: 80rpx;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 24rpx;
  text-align: center;
  color:	#696969;
}

.kongGe {
  width: 180rpx;
}